{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Quartz": {
    "NoWalkJob": "0 0 0 * * ?",
    "ActualUsernameJob": "0 0 0/3 * * ?",
    "RestrictUserAccessJob": "0 0/5 * * * ?",
    "ClearMessagesJob": "0 0/10 * * * ?"
  },
  "Configuration": {
    "PeriodEchoSec": 60,
    "PeriodResetAccessMin": 60,
    "PeriodChatListMin": 1,
    "PeriodClearPrivateChatMin": 10,
    "PeriodClearGroupChatHours": 336,
    "PeriodInactiveUserDays": 7,
    "WalkingTime": "08:40",
    "BanDays": "None"
  },
  "ConnectionStrings": {
    "DynamoConnectionString": "AccessKeyID:*;SecretAccessKey:*;Region:us-east-2",
    "MongoDbConnectionString": "mongodb://localhost:27017"
  }
}
